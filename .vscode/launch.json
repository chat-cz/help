{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "MkDocs: serve (venv)",
      "type": "debugpy",
      "request": "launch",
      "module": "mkdocs",
      "args": [
        "serve",
        "-w",
        "./",
        "--livereload"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "PATH": "${workspaceFolder}/.venv/bin:${env:PATH}"
      }
    }
  ]
}
